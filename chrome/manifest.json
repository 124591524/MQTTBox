{
  "manifest_version": 2,
  "name": "Mqtt Client",
  "short_name": "Mqtt Client",
  "description": "Developers helper program to create and test MQTT connectivity protocol.",
  "version": "0.0.9",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "img/icon-16.png",
    "128": "img/icon-128.png"
  },
  "app": {
    "background": {
      "scripts": ["./js/background_chrome.js"]
    }
  },
  "sockets": {
    "tcp" : {
      "connect": ["*"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ]
}
